{
  "name": "levenmorpher",
  "version": "1.2.2",
  "description": "Morph one word into another, one word at a time.",
  "license": "MIT",
  "keywords": [
    "levenshtein",
    "array",
    "words",
    "fun",
    "game"
  ],
  "repository": "words/levenmorpher",
  "bugs": "https://github.com/words/levenmorpher/issues",
  "author": "Zeke Sikelianos <zeke@sikelianos.com> (http://zeke.sikelianos.com)",
  "contributors": [
    "Zeke Sikelianos <zeke@sikelianos.com> (http://zeke.sikelianos.com)",
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "main": "index.js",
  "bin": {
    "morph": "cli.js",
    "levenmorpher": "cli.js"
  },
  "files": [
    "cli.js",
    "index.js"
  ],
  "dependencies": {
    "levenshtein-edit-distance": "^2.0.0",
    "similar-english-words": "^1.0.0"
  },
  "devDependencies": {
    "prettier": "^1.0.0",
    "remark-cli": "^7.0.0",
    "remark-preset-wooorm": "^6.0.0",
    "tape": "^4.0.0",
    "xo": "^0.25.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier --write \"**/*.js\" && xo --fix",
    "test-api": "node test",
    "test": "npm run format && npm run test-api"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
